{
  "name": "@embedai/web-sdk",
  "version": "1.0.0",
  "description": "EmbedAI Web SDK - 智能聊天助手嵌入式组件",
  "main": "dist/embedai.min.js",
  "module": "dist/embedai.esm.js",
  "types": "dist/types/index.d.ts",
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "build:types": "tsc --declaration --emitDeclarationOnly --outDir dist/types",
    "preview": "vite preview",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "format": "prettier --write src/**/*.{ts,tsx,css,scss}",
    "prepare": "npm run build"
  },
  "keywords": [
    "embedai",
    "chatbot",
    "ai",
    "chat",
    "widget",
    "sdk",
    "javascript",
    "typescript"
  ],
  "author": "EmbedAI Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/congwa/EmbedAI-sdk.git"
  },
  "bugs": {
    "url": "https://github.com/congwa/EmbedAI-sdk/issues"
  },
  "homepage": "https://github.com/congwa/EmbedAI-sdk#readme",
  "devDependencies": {
    "@types/jest": "^29.5.5",
    "@typescript-eslint/eslint-plugin": "^6.7.4",
    "@typescript-eslint/parser": "^6.7.4",
    "eslint": "^8.50.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^29.7.0",
    "prettier": "^3.0.3",
    "ts-jest": "^29.1.1",
    "typescript": "^5.2.2",
    "vite": "^4.4.9",
    "vite-plugin-dts": "^3.6.0"
  },
  "dependencies": {},
  "peerDependencies": {},
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}
